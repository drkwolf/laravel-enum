{
    "name": "mad-web/laravel-enum",
    "description": "Elegant Enum implementation for Laravel. With make:enum command, ability to casting for Eloquent models and labels translations via lang files.",
    "type": "library",
    "keywords": [
        "mad-web",
        "enum",
        "laravel-enum"
    ],
    "homepage": "https://github.com/mad-web/laravel-enum",
    "license": "MIT",
    "authors": [
        {
            "name": "mad-web",
            "email": "madweb.dev@gmail.com",
            "homepage": "https://github.com/mad-web",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "~5.6.0|~5.7.0|~5.8.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0",
        "illuminate/translation": "~5.6.0|~5.7.0|~5.8.0",
        "myclabs/php-enum": "^1.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.0.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0",
        "phpunit/phpunit": "^6.5|^7.3"
    },
    "autoload": {
        "psr-4": {
            "MadWeb\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadWeb\\Enum\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadWeb\\Enum\\EnumServiceProvider"
            ]
        }
    }
}
